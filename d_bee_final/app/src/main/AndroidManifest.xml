<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.d_bee_final">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.D_bee_final"
        android:usesCleartextTraffic="true">
        <activity android:name=".ui.mine_page.my_sharing.share_article.ShareArticleActivity"></activity>
        <activity android:name=".ui.mine_page.my_sharing.MySharingActivity" />
        <activity android:name=".ui.mine_page.collected_list.MyCollectActivity" />
        <activity android:name=".ui.mine_page.my_coin_page.MyCoinActivity" />
        <activity
            android:name=".ui.home_page.search.SearchActivity"
            android:launchMode="singleTop" />
        <activity android:name=".ui.structure_page.classified_structure_page.StructureArticleActivity" />
        <activity
            android:name=".ui.login_page.LoginActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.WebActivity" />
        <activity android:name=".ui.MainActivity" />
    </application>

</manifest>